<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GreensFridge">

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="414dp"
        android:layout_height="920dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343"
        app:srcCompat="@drawable/fridge_second" />

    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="311dp"
        android:layout_height="108dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/Cucumber"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="168dp"
        android:clickable="true"
        android:onClick="openFragment"
        app:layout_constraintStart_toEndOf="@+id/banana"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:srcCompat="@drawable/cucumber" />

    <ImageView
        android:id="@+id/banana"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="116dp"
        android:clickable="true"
        android:onClick="openFragment"
        app:layout_constraintStart_toEndOf="@+id/apple"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/banana" />

    <ImageView
        android:id="@+id/tomato"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="116dp"
        android:clickable="true"
        android:onClick="openFragment"
        app:layout_constraintStart_toEndOf="@+id/potato"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tomat" />

    <ImageView
        android:id="@+id/apple"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="116dp"
        android:clickable="true"
        android:onClick="openFragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/apple" />

    <ImageView
        android:id="@+id/potato"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="116dp"
        android:clickable="true"
        android:onClick="openFragment"
        app:layout_constraintStart_toEndOf="@+id/Cucumber"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kartoffel" />

</androidx.constraintlayout.widget.ConstraintLayout>