<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/BackToMainShop"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MeatShop">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meat shop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="181dp" />

    <FrameLayout
        android:id="@+id/animationContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    </FrameLayout>

    <ImageView
        android:id="@+id/Background"
        android:layout_width="413dp"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseshoptest" />

    <Button
        android:id="@+id/button5"
        android:layout_width="90dp"
        android:layout_height="147dp"
        android:layout_marginStart="116dp"
        android:layout_marginEnd="1dp"
        android:alpha="0"
        android:clickable="true"
        android:onClick="OpenGreensShop"
        android:text="Button"
        app:layout_constraintEnd_toStartOf="@+id/button9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button11"
        android:layout_width="84dp"
        android:layout_height="129dp"
        android:layout_marginTop="16dp"
        android:alpha="0"
        android:clickable="true"
        android:onClick="OpenDairyShop"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.097"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/button10"
        android:layout_width="96dp"
        android:layout_height="147dp"
        android:layout_marginEnd="20dp"
        android:alpha="0"
        android:onClick="OpenDiverseShop"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button9" />

    <Button
        android:id="@+id/buttonGoToMainShop"
        android:layout_width="86dp"
        android:layout_height="131dp"
        android:layout_marginTop="16dp"
        android:alpha="0"
        android:onClick="goToMainShop"
        android:text="Go to Main Shop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.643"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <GridView
        android:id="@+id/gridViewMeat"
        android:layout_width="334dp"
        android:layout_height="506dp"
        android:layout_marginBottom="129dp"
        android:numColumns="3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <Button
        android:id="@+id/button7"
        android:layout_width="128dp"
        android:layout_height="75dp"
        android:alpha="0"
        android:onClick="goToChecklist"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="@+id/Background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>